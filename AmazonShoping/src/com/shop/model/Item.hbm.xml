<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.shop.model.Item" table="item_table" lazy="true">

		<id name="id">
			<generator class="native" />
		</id>

		<property name="userId" ></property>
		<property name="time" ></property>
		<property name="status" ></property>
		<property name="address" ></property>
		<property name="sendCompany" ></property>
		<property name="typeStatus" ></property>
	    <property name="productId" ></property>
	    <property name="totalPrice"></property>

	</class>

</hibernate-mapping>